{
  "name": "cab-killer",
  "version": "1.0.0",
  "description": "A multi-agent code review system that independently reviews code written by coding agents",
  "type": "module",
  "main": "src/index.js",
  "bin": {
    "cab-killer": "./src/index.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "test": "node --test tests/**/*.test.js",
    "test:watch": "node --test --watch tests/**/*.test.js",
    "lint": "eslint src/ tests/",
    "lint:fix": "eslint src/ tests/ --fix",
    "format": "prettier --write 'src/**/*.js' 'tests/**/*.js' '*.js'",
    "format:check": "prettier --check 'src/**/*.js' 'tests/**/*.js' '*.js'"
  },
  "keywords": [
    "code-review",
    "ai",
    "claude",
    "multi-agent",
    "static-analysis"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.2.22",
    "commander": "^13.0.0",
    "glob": "^11.0.0"
  },
  "devDependencies": {
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.8.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
